  "confidence_scoring": {
    "http_status_weight": 0.30,
    "content_analysis_weight": 0.25,
    "response_time_weight": 0.15,
    "url_structure_weight": 0.15,
    "title_analysis_weight": 0.15,
    "thresholds": {
      "found": 70,
      "maybe": 40,
      "not_found": 0
    }
  },
  "username_variations": {
    "enabled": true,
    "max_variations": 50,
    "prefixes": [
      "the", "real", "official", "iam", "im", "its", 
      "mr", "mrs", "ms", "dr", "prof"
    ],
    "suffixes": [
      "official", "real", "verified", "pro", "dev", 
      "admin", "mod", "123", "2024", "2025"
    ],
    "separators": ["_", ".", "-"],
    "number_range": [1, 99]
  },
  "email_analysis": {
    "enabled": true,
    "check_mx_records": true,
    "check_breaches": true,
    "breach_api": "haveibeenpwned"
  },
  "phone_analysis": {
    "enabled": true,
    "validate_format": true,
    "detect_country": true,
    "detect_type": true
  },
  "domain_analysis": {
    "enabled": true,
    "dns_records": ["A", "AAAA", "MX", "NS", "TXT", "SOA"],
    "whois_lookup": true,
    "ssl_check": true,
    "ip_resolution": true
  }
}